<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>{{PackageName}}</id>
    <title>Fitbit Ultra</title>
    <version>{{PackageVersion}}</version>
    <authors>Fitbit.com</authors>
    <owners>Jivko Kolev</owners>
    <projectUrl>http://www.fitbit.com</projectUrl>
    <licenseUrl>http://www.fitbit.com</licenseUrl>
	<iconUrl>https://raw.github.com/jivkok/Chocolatey-AutomaticPackages/master/Fitbit.Ultra/Fitbit.Ultra.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Fitbit Ultra - uploader for your daily activity stats (steps, distance) from your Fitbit Ultra tracker</summary>
    <description>Fitbit's Uploader software uploads your Ultra Tracker's stats (steps, distance) from your base station to your personalized dashboard on Fitbit.com, so you can track your activity, log food, and much more.</description>
    <tags>fitbit ultra tracker</tags>
    <dependencies>
      <dependency id='autoit.commandline' />
    </dependencies>
  </metadata>
</package>
